void main() {
  var arr = [5, 8, 2, 4, 0, 32, 2, 4, 7, 90, 2, 1, 5];

  int n = arr[0]; // Инициализируем максимальное значение первым элементом массива

  for (int i = 1; i < arr.length; i++) {
    if (arr[i] > n) {
      n = arr[i]; // Обновляем максимум, если текущий элемент больше текущего максимума
    }
  }

  print('Максимум: $n'); // выведет 90
}
